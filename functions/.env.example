# Calendado Functions Environment Variables
# These are used for local development and testing
# In production, these values are stored in Google Secret Manager

# Resend Configuration
RESEND_API_KEY=re_your_resend_api_key_here
RESEND_WEBHOOK_SECRET=your_webhook_secret_here

# Email Configuration
FROM_EMAIL=hello@updates.calendado.com
FROM_NAME=Calendado Updates

# Application Configuration
APP_BASE_URL=https://calendado.com

# Development Settings
NODE_ENV=development
FUNCTIONS_EMULATOR=true

# Firebase Project (for local testing)
FIREBASE_PROJECT_ID=calendado-dev
